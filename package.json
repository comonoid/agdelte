{
  "name": "agdelte",
  "version": "0.1.0",
  "description": "Reactive UI Framework with Dependent Types in Agda",
  "type": "module",
  "main": "runtime/index.js",
  "exports": {
    ".": "./runtime/index.js",
    "./dom": "./runtime/dom.js",
    "./events": "./runtime/events.js",
    "./primitives": "./runtime/primitives.js"
  },
  "scripts": {
    "test": "node test/runtime.test.js",
    "build:counter": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Counter.agda",
    "build:timer": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Timer.agda",
    "build:todo": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Todo.agda",
    "build:keyboard": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Keyboard.agda",
    "build:http": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Http.agda",
    "build:task": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Task.agda",
    "build:websocket": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/WebSocket.agda",
    "build:router": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Router.agda",
    "build:composition": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Composition.agda",
    "build:transitions": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Transitions.agda",
    "build:combinators": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Combinators.agda",
    "build:optic-dynamic": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/OpticDynamic.agda",
    "build:all": "npm run build:counter && npm run build:timer && npm run build:todo && npm run build:keyboard && npm run build:http && npm run build:task && npm run build:websocket && npm run build:router && npm run build:composition && npm run build:transitions && npm run build:combinators && npm run build:optic-dynamic",
    "typecheck": "agda -i src src/Agdelte/Reactive/Node.agda",
    "typecheck:theory": "agda -i src src/Agdelte/Theory/Poly.agda",
    "typecheck:all": "agda -i src src/Agdelte/Reactive/Node.agda && agda -i src src/Agdelte/Theory/Poly.agda && agda -i src -i examples examples/Counter.agda && agda -i src -i examples examples/Timer.agda && agda -i src -i examples examples/Todo.agda",
    "dev": "node serve.cjs",
    "clean": "rm -rf _build MAlonzo jAgda"
  },
  "keywords": [
    "agda",
    "reactive",
    "ui",
    "framework",
    "dependent-types",
    "polynomial-functors",
    "svelte-style",
    "no-virtual-dom"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {},
  "agda": {
    "version": ">=2.6.4",
    "standardLibrary": ">=2.0"
  }
}
