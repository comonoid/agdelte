{
  "name": "agdelte",
  "version": "0.1.0",
  "description": "Reactive UI Framework with Dependent Types in Agda",
  "type": "module",
  "main": "runtime/index.js",
  "exports": {
    ".": "./runtime/index.js",
    "./dom": "./runtime/dom.js",
    "./events": "./runtime/events.js",
    "./primitives": "./runtime/primitives.js"
  },
  "scripts": {
    "test": "node test/runtime.test.js",
    "build:counter": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Counter.agda",
    "build:timer": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Timer.agda",
    "build:todo": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/Todo.agda",
    "build:keyboard": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/KeyboardDemo.agda",
    "build:http": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/HttpDemo.agda",
    "build:task": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/TaskDemo.agda",
    "build:websocket": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/WebSocketDemo.agda",
    "build:router": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/RouterDemo.agda",
    "build:composition": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/CompositionDemo.agda",
    "build:widgetcounter": "agda --js --js-es6 --js-optimize --compile-dir=_build -i src -i examples examples/WidgetCounter.agda",
    "build:all": "npm run build:counter && npm run build:timer && npm run build:todo && npm run build:keyboard && npm run build:http && npm run build:task && npm run build:websocket && npm run build:router && npm run build:composition && npm run build:widgetcounter",
    "typecheck": "agda -i src src/Agdelte/App.agda",
    "typecheck:theory": "agda -i src src/Agdelte/Theory/PolyApp.agda",
    "typecheck:all": "agda -i src -i examples src/Agdelte/App.agda && agda -i src src/Agdelte/Theory/PolyApp.agda && agda -i src -i examples examples/Counter.agda && agda -i src -i examples examples/Timer.agda && agda -i src -i examples examples/Todo.agda",
    "dev": "node serve.cjs",
    "clean": "rm -rf _build MAlonzo jAgda"
  },
  "keywords": [
    "agda",
    "reactive",
    "ui",
    "framework",
    "dependent-types",
    "polynomial-functors",
    "elm-architecture"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {},
  "agda": {
    "version": ">=2.6.4",
    "standardLibrary": ">=2.0"
  }
}
